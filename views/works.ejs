<%- include('partials/header.ejs') %>

<ul class="works">
    <% if(isler.length>0) { %>

        <% isler.forEach(is=>{ %>
    <li class="work">    
        <img src="/work.png" />
        <h4><%= is.baslik %></h4>
        <h5><%= is.aciklama%></h5>
        <button class="delete-btn" onclick="deleteWork"><i class="fa fa-trash"></i></button>
       
    </li>
    <% }) %>
    <% } else { %>
        <div> Henüz iş girişi yapılmadı</div>
        <% } %>
</ul>
<button class="btn"><a href="/work-add">İş ekle</a></button>

<%- include('partials/footer.ejs') %>